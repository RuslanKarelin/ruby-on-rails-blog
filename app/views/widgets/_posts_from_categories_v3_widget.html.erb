<div class="section">
  <div class="container">
    <div class="row">
      <% get_posts(9).in_groups_of(3) do |group| %>
        <div class="col-md-4">
          <%= render partial: 'posts/post_v6', collection: group, as: :post, cached: true %>
        </div>
      <% end %>
    </div>
  </div>
</div>
