<tr class="jsgrid-row">
  <td class="jsgrid-cell"><%= role.id %></td>
  <td class="jsgrid-cell"><%= role.name %></td>
  <td class="jsgrid-cell">&nbsp;
    <%= link_to edit_admin_role_path(role) do %>
      <i class="fa fa-edit"></i>
    <% end %>
    &nbsp;
    <%= link_to admin_role_path(role), method: :delete, data: {:confirm => 'Are you sure?'} do %>
      <i class="fa fa-trash"></i>
    <% end %>
  </td>
</tr>